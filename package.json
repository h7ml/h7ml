{
  "name": "h7ml",
  "version": "2.0.1",
  "author": "h7ml <h7ml@qq.com>",
  "description": "每一个不曾起舞的日子,都是对生命的辜负!",
  "license": "MIT",
  "scripts": {
    "cz": "czg",
    "build": "vuepress build h7ml",
    "clean-build": "vuepress build h7ml --clean-cache",
    "clean-dev": "vuepress dev h7ml --clean-cache",
    "dev": "vuepress dev h7ml",
    "server": "node ./script/server.js",
    "start": "node ./script/server.js",
    "clean:crawler": "node ./script/crawler/clean.js",
    "clean:test": "node ./script/crawler/test.js",
    "crawler:segmentfault": "node ./script/crawler/segmentfault.js",
    "crawler:juejin": "node ./script/crawler/juejin.js",
    "crawler:jianshu": "node ./script/crawler/jianshu.js",
    "crawler:csdn": "node ./script/crawler/csdn.js",
    "crawler:copy": "node ./script/crawler/copy.js",
    "crawler:interview": "node ./script/crawler/interview.js",
    "crawler:sitemap2txt": "node ./script/crawler/sitemap2txt.js",
    "crawler": "npm-run-all --parallel crawler:* --serial lint",
    "lint": "prettier --check --write .",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,json}\"",
    "eslint:fix": "npx eslint --cache --fix --ext .js,.jsx,.ts,.tsx ./h7ml",
    "lint:check": "prettier --check .",
    "layer0:dev": "layer0 dev",
    "layer0:build": "vuepress build h7ml -d=h7ml/.vuepress/dist",
    "layer0:deploy": "layer0 deploy",
    "heroku-postbuild": "node ./script/server.js",
    "lint-md": "markdownlint **/*.md --ignore node_modules --fix",
    "updates": "curl -o README.md https://nest.h7ml.cn/api/github.md && pnpm dlx vp-update && pnpm i && pnpm up"
  },
  "keywords": [
    "vuepress",
    "h7ml",
    "h7ml@qq.com"
  ],
  "devDependencies": {
    "@dext7r/cz-config": "^1.0.4",
    "@dext7r/eslint-config": "^1.0.0",
    "@dext7r/prettier": "^0.0.9",
    "@layer0/cli": "^4.20.0",
    "@layer0/core": "^4.20.0",
    "@layer0/devtools": "^4.20.0",
    "@layer0/prefetch": "^4.20.0",
    "@types/node": "^18.18.10",
    "@vuepress/bundler-vite": "2.0.0-rc.0",
    "@vuepress/cli": "2.0.0-rc.0",
    "@vuepress/client": "2.0.0-rc.0",
    "@vuepress/plugin-docsearch": "2.0.0-rc.0",
    "@vuepress/plugin-google-analytics": "2.0.0-rc.0",
    "@vuepress/plugin-register-components": "2.0.0-rc.0",
    "@vuepress/utils": "2.0.0-rc.0",
    "axios": "^1.6.2",
    "commitizen": "^4.3.0",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "cz-customizable": "^7.0.0",
    "cz-git": "^1.7.1",
    "czg": "^1.7.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "eslint": "8.35.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "expect-puppeteer": "^8.0.5",
    "husky": "^8.0.3",
    "lint-staged": "13.1.2",
    "lodash": "^4.17.21",
    "markdownlint-cli": "0.33.0",
    "node-json-db": "^2.3.0",
    "node-unzip-2": "0.2.8",
    "nodemailer": "^6.9.7",
    "npm-check-updates": "^16.14.6",
    "npm-run-all2": "^6.1.1",
    "open-url-by-browser": "1.0.1",
    "prettier": "2.8.4",
    "puppeteer": "^19.11.1",
    "pushstate-server": "3.1.0",
    "qrcode": "^1.5.3",
    "rimraf": "^4.4.1",
    "rss-parser": "^3.13.0",
    "shelljs": "0.8.5",
    "turndown": "^7.1.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-compression2": "^0.8.4",
    "vitepress-plugin-compression": "^0.1.4",
    "vue": "^3.3.8",
    "vuepress": "2.0.0-rc.0",
    "vuepress-plugin-auto-catalog": "2.0.0-rc.0",
    "vuepress-plugin-blog2": "2.0.0-rc.0",
    "vuepress-plugin-china-search-console": "2.1.5",
    "vuepress-plugin-comment2": "2.0.0-rc.0",
    "vuepress-plugin-components": "2.0.0-rc.0",
    "vuepress-plugin-copy-code2": "2.0.0-rc.0",
    "vuepress-plugin-copyright2": "2.0.0-rc.0",
    "vuepress-plugin-feed2": "2.0.0-rc.0",
    "vuepress-plugin-md-enhance": "2.0.0-rc.0",
    "vuepress-plugin-photo-swipe": "2.0.0-rc.0",
    "vuepress-plugin-pwa2": "2.0.0-rc.0",
    "vuepress-plugin-reading-time2": "2.0.0-rc.0",
    "vuepress-plugin-rtl": "2.0.0-rc.0",
    "vuepress-plugin-sass-palette": "2.0.0-rc.0",
    "vuepress-plugin-search-pro": "2.0.0-rc.0",
    "vuepress-plugin-seo2": "2.0.0-rc.0",
    "vuepress-plugin-sitemap2": "2.0.0-rc.0",
    "vuepress-shared": "2.0.0-beta.193",
    "vuepress-theme-hope": "2.0.0-rc.0"
  },
  "packageManager": "pnpm@7.3.0",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "@types/react",
        "react",
        "react-dom",
        "webpack",
        "@layer0/core",
        "vuepress-theme-hope"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "husky": {
    "hooks": {
      "prettier": "prettier",
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,html,ts,jsx,tsx,vue}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@vue/repl": "^2.7.0",
    "chart.js": "^4.4.0",
    "dashjs": "^4.7.2",
    "echarts": "^5.4.3",
    "flowchart.ts": "^1.0.1",
    "hls.js": "^1.4.12",
    "mathjax-full": "^3.2.2",
    "mermaid": "^10.6.1",
    "mpegts.js": "^1.7.3",
    "plyr": "^3.7.8",
    "vidstack": "v1"
  }
}
